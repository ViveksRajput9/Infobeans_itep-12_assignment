package Practiseteach;

public class Q1 {
	public static void main(String[] args) {
		String a = "  vivek singh rajput   ";
		System.out.println(a.length());
		a=a.trim();
		System.out.println(a);
		System.out.println(a.length());
		a=a.replace("a","D");
		System.out.println(a.length());
		System.out.println(a);
	}
}